{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "TuringTrader.Simulator/**.csproj"
          ],
	  "exclude": [
		  "**/bin/**",
		  "**/obj/**",
		  "_site/**"
	  ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "articles/overview/**.md",
          "articles/overview/**/toc.yml",
          "articles/qsg/**.md",
          "articles/qsg/**/toc.yml",
          "articles/qsg/v1/**.md",
          "articles/qsg/v1/**/toc.yml",
          "articles/qsg/v2/**.md",
          "articles/qsg/v2/**/toc.yml",
          "toc.yml",
          "*.md",
	        "index.html"
        ]
      },
      {
         "files": [
          "docnav/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/turingtrader"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap": {
	    "baseUrl": "https://www.turingtrader.org/help"
    }
  }
}
